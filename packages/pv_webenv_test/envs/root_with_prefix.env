# Main environment configuration
APP_NAME=PV WebEnv Test with Prefix
APP_VERSION=1.0.1
DEBUG_MODE=true

# File references - these will trigger loading of other config files (existing functionality)
DATABASE_CONFIG={{database.json}}
API_CONFIG={{api.toml}}
CACHE_CONFIG={{cache.env}}

# New prefix functionality tests - these will load files with prefixes
DB=<<database_prefixed.env>>
AUTH=<<auth_service.json>>
MONITOR=<<monitoring.toml>>

# Direct values
MAX_RETRIES=3
TIMEOUT_SECONDS=30 