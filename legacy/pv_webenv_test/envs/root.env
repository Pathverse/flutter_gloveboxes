# Main environment configuration
APP_NAME=PV WebEnv Test
APP_VERSION=1.0.0
DEBUG_MODE=true

# File references - these will trigger loading of other config files
DATABASE_CONFIG={{database.json}}
API_CONFIG={{api.toml}}
